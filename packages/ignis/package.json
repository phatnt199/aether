{
  "name": "@minimaltech/ignis",
  "version": "0.1.0",
  "description": "Minimal Technology - NodeJS Infrastructure - Hono Framework (Class-based with Decorators)",
  "keywords": [
    "web",
    "restful",
    "rest",
    "api",
    "hono",
    "auth",
    "security",
    "backend",
    "framework",
    "backend framework",
    "infrastructure",
    "minimaltech",
    "minimaltek",
    "mt",
    "infra",
    "nodejs",
    "nodejs framework",
    "decorator",
    "class-based",
    "dependency injection"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "sh ./scripts/build.sh",
    "clean": "sh ./scripts/clean.sh",
    "eslint": "eslint --report-unused-disable-directives .",
    "lint": "bun run eslint && bun run prettier:cli",
    "lint:fix": "bun run eslint --fix && bun run prettier:fix",
    "prettier:cli": "prettier \"**/*.{js,ts}\" -l",
    "prettier:fix": "bun run prettier:cli --write",
    "rebuild": "sh ./scripts/rebuild.sh no-version",
    "rebuild:major": "sh ./scripts/rebuild.sh major",
    "rebuild:minor": "sh ./scripts/rebuild.sh minor",
    "rebuild:patch": "sh ./scripts/rebuild.sh patch",
    "rebuild:premajor": "sh ./scripts/rebuild.sh premajor",
    "rebuild:preminor": "sh ./scripts/rebuild.sh preminor",
    "rebuild:prepatch": "sh ./scripts/rebuild.sh prepatch",
    "rebuild:prerelease": "sh ./scripts/rebuild.sh prerelease",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phatnt199/aether.git"
  },
  "author": "Minimal Technology <developer@miminaltek.com>",
  "bugs": {
    "url": "https://github.com/phatnt199/aether/issues"
  },
  "homepage": "https://github.com/phatnt199/aether/wiki",
  "license": "MIT",
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "!/**/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "@hono/node-server": "^1.13.7",
    "@node-oauth/oauth2-server": "^5.2.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "axios": "^1.12.2",
    "bullmq": "^5.58.7",
    "casbin": "^5.38.0",
    "cron": "^4.3.3",
    "dayjs": "^1.11.18",
    "dotenv-flow": "^4.1.0",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.6.14",
    "ioredis": "^5.7.0",
    "jose": "^5.9.6",
    "lodash": "^4.17.21",
    "minio": "^8.0.6",
    "mqtt": "^5.14.1",
    "multer": "^2.0.2",
    "postgres": "^3.4.5",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tsyringe": "^4.8.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.9.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.2",
    "@minimaltech/eslint-node": "^0.2.1",
    "drizzle-kit": "^0.28.1",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "vitest": "^2.1.8"
  },
  "sideEffects": false
}
