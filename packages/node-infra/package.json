{
  "name": "@minimaltech/node-infra",
  "version": "0.5.10-1",
  "description": "Minimal Technology - NodeJS Infrastructure - Loopback 4 Framework",
  "keywords": [
    "web",
    "restful",
    "rest",
    "api",
    "express",
    "auth",
    "security",
    "backend",
    "framework",
    "backend framework",
    "StrongLoop",
    "loopback",
    "loopback-application",
    "infrastructure",
    "minimaltech",
    "minimaltek",
    "mt",
    "infra",
    "nodejs",
    "nodejs framework"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./lb-auth": {
      "default": "./dist/base/loopback/@lb/auth/index.js",
      "types": "./dist/base/loopback/@lb/auth/index.d.ts"
    },
    "./lb-core": {
      "default": "./dist/base/loopback/@lb/core/index.js",
      "types": "./dist/base/loopback/@lb/core/index.d.ts"
    },
    "./lb-rest": {
      "default": "./dist/base/loopback/@lb/rest/index.js",
      "types": "./dist/base/loopback/@lb/rest/index.d.ts"
    },
    "./lb-repo": {
      "default": "./dist/base/loopback/@lb/repository/index.js",
      "types": "./dist/base/loopback/@lb/repository/index.d.ts"
    },
    "./@lb/auth": {
      "default": "./dist/base/loopback/@lb/auth/index.js",
      "types": "./dist/base/loopback/@lb/auth/index.d.ts"
    },
    "./@lb/core": {
      "default": "./dist/base/loopback/@lb/core/index.js",
      "types": "./dist/base/loopback/@lb/core/index.d.ts"
    },
    "./@lb/rest": {
      "default": "./dist/base/loopback/@lb/rest/index.js",
      "types": "./dist/base/loopback/@lb/rest/index.d.ts"
    },
    "./@lb/repo": {
      "default": "./dist/base/loopback/@lb/repository/index.js",
      "types": "./dist/base/loopback/@lb/repository/index.d.ts"
    },
    "./configs/tsconfig.common.json": "./configs/tsconfig.common.json"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "sh ./scripts/build.sh",
    "clean": "sh ./scripts/clean.sh",
    "eslint": "eslint --report-unused-disable-directives .",
    "lint": "pnpm eslint && pnpm prettier:cli",
    "lint:fix": "pnpm eslint --fix && pnpm prettier:fix",
    "prettier:cli": "prettier \"**/*.{js,ts}\" -l",
    "prettier:fix": "pnpm prettier:cli --write",
    "rebuild": "sh ./scripts/rebuild.sh no-version",
    "rebuild:major": "sh ./scripts/rebuild.sh major",
    "rebuild:minor": "sh ./scripts/rebuild.sh minor",
    "rebuild:patch": "sh ./scripts/rebuild.sh patch",
    "rebuild:premajor": "sh ./scripts/rebuild.sh premajor",
    "rebuild:preminor": "sh ./scripts/rebuild.sh preminor",
    "rebuild:prepatch": "sh ./scripts/rebuild.sh prepatch",
    "rebuild:prerelease": "sh ./scripts/rebuild.sh prerelease",
    "pretest:develop": "pnpm run rebuild",
    "test:develop": "NODE_ENV=development mocha -r dotenv-flow/config -r source-map-support/register --timeout 60000 --exit dist/__tests__/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phatnt199/aether.git"
  },
  "author": "Minimal Technology <developer@miminaltek.com>",
  "bugs": {
    "url": "https://github.com/phatnt199/aether/issues"
  },
  "homepage": "https://github.com/phatnt199/aether/wiki",
  "license": "MIT",
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "configs",
    "!/**/tests",
    "!/**/dist/tests",
    "!/**/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@minimaltech/common": "workspace:*",
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "@loopback/authentication": "^12.0.6",
    "@loopback/authentication-jwt": "^0.16.6",
    "@loopback/authorization": "^0.16.5",
    "@loopback/boot": "^8.0.5",
    "@loopback/core": "^7.0.4",
    "@loopback/health": "^0.15.5",
    "@loopback/metadata": "^8.0.4",
    "@loopback/repository": "^8.0.4",
    "@loopback/rest": "^15.0.5",
    "@loopback/rest-crud": "^0.19.5",
    "@loopback/rest-explorer": "^8.0.5",
    "@loopback/security": "^0.12.5",
    "@loopback/service-proxy": "^8.0.4",
    "@node-oauth/oauth2-server": "^5.2.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "axios": "^1.12.2",
    "bullmq": "^5.58.7",
    "casbin": "^5.38.0",
    "cron": "^4.3.3",
    "dayjs": "^1.11.18",
    "dotenv-flow": "^4.1.0",
    "ejs": "^3.1.10",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "minio": "^8.0.6",
    "mqtt": "^5.14.1",
    "multer": "^2.0.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.9.0"
  },
  "peerDependenciesMeta": {
    "mocha": {
      "optional": true
    }
  },
  "devDependencies": {
    "@loopback/build": "^12.0.4",
    "@minimaltech/eslint-node": "^0.2.1",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/mocha": "^10.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.2",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.3"
  },
  "sideEffects": false
}
