{
  "name": "@minimaltech/node-infra",
  "version": "0.2.34",
  "description": "NodeJS Infrastructure - Loopback 4 Framework",
  "keywords": [
    "web",
    "restful",
    "rest",
    "api",
    "express",
    "auth",
    "security",
    "backend",
    "framework",
    "backend framework",
    "StrongLoop",
    "loopback",
    "loopback-application",
    "infrastructure"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "sh ./scripts/build.sh",
    "build:watch": "lb-tsc --watch",
    "clean": "sh ./scripts/clean.sh",
    "eslint": "eslint --report-unused-disable-directives .",
    "lint": "pnpm eslint && pnpm prettier:cli",
    "lint:fix": "pnpm eslint --fix && pnpm prettier:fix",
    "prettier:cli": "prettier \"**/*.{js,ts}\" -l",
    "prettier:fix": "pnpm prettier:cli --write",
    "rebuild": "sh ./scripts/rebuild.sh no-version",
    "rebuild:major": "sh ./scripts/rebuild.sh major",
    "rebuild:minor": "sh ./scripts/rebuild.sh minor",
    "rebuild:patch": "sh ./scripts/rebuild.sh patch",
    "pretest:dev": "pnpm run rebuild",
    "test:dev": "NODE_ENV=development mocha -r dotenv-flow/config -r source-map-support/register --timeout 60000 --exit dist/__tests__/index.js",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phatnt199/node-infra.git"
  },
  "author": "Minimal Technology <developer@miminaltek.com>",
  "bugs": {
    "url": "https://github.com/phatnt199/node-infra/issues"
  },
  "homepage": "https://github.com/phatnt199/node-infra/wiki",
  "license": "MIT",
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "!/**/tests",
    "!/**/dist/tests",
    "!/**/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.1",
    "@grpc/proto-loader": "^0.7.13",
    "@loopback/authentication": "^11.0.3",
    "@loopback/authentication-jwt": "^0.15.3",
    "@loopback/authentication-passport": "^7.0.3",
    "@loopback/authorization": "^0.15.3",
    "@loopback/boot": "^7.0.3",
    "@loopback/core": "^6.1.0",
    "@loopback/http-server": "^6.0.3",
    "@loopback/metadata": "^7.0.5",
    "@loopback/repository": "^7.0.3",
    "@loopback/rest": "^14.0.3",
    "@loopback/rest-crud": "^0.18.3",
    "@loopback/rest-explorer": "^7.0.3",
    "@loopback/security": "^0.11.3",
    "@loopback/service-proxy": "^7.0.3",
    "@node-oauth/oauth2-server": "^5.1.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "axios": "^1.7.7",
    "bullmq": "^5.12.12",
    "casbin": "^5.30.0",
    "cron": "^3.1.7",
    "dayjs": "^1.11.12",
    "dotenv-flow": "4.1.0",
    "ejs": "^3.1.10",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "minio": "^8.0.1",
    "mocha": "^10.7.3",
    "mqtt": "^5.9.0",
    "multer": "^1.4.5-lts.1",
    "rxjs": "^7.8.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.3",
    "@loopback/testlab": "^7.0.3",
    "@minimaltech/eslint-node": "^0.1.14",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/mocha": "^10.0.7",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.16.2",
    "eslint": "^8.57.0",
    "husky": "^9.1.5",
    "prettier": "^3.3.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  }
}
